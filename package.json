{
  "name": "coc-fzf-preview",
  "version": "0.4.27",
  "description": "The plugin that powerfully integrates fzf and (Neo)vim. It is also possible to integrate with coc.",
  "author": "yuki-yano",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:yuki-ycino/fzf-preview.vim"
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "autoload",
    "plugin",
    "bin"
  ],
  "keywords": [
    "coc.nvim",
    "fzf"
  ],
  "engines": {
    "coc": "^0.0.79"
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:remote": "yarn run clean && TS_NODE_PROJECT=\"tsconfig.webpack-jest.json\" yarn run webpack --config webpack.remote.ts",
    "build:coc": "yarn run clean && TS_NODE_PROJECT=\"tsconfig.webpack-jest.json\" yarn run webpack --config webpack.coc.ts",
    "dev": "npm-run-all --parallel dev:*",
    "dev:remote": "yarn run clean && TS_NODE_PROJECT=\"tsconfig.webpack-jest.json\" yarn run webpack --watch --config webpack.remote.ts",
    "dev:coc": "yarn run clean && TS_NODE_PROJECT=\"tsconfig.webpack-jest.json\" yarn run webpack --watch --config webpack.coc.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext --fix .ts src",
    "lint:watch": "esw --watch --color --clear --ext .ts src",
    "test": "jest",
    "test:watch": "jest --watch",
    "cli": "ts-node --files -r tsconfig-paths/register",
    "clean": "rimraf lib rplugin/node/fzf-preview.vim/index.js",
    "postinstall": "yarn run build:remote"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "ansi-escape-sequences": "^5.1.2",
    "camelcase": "^6.2.0",
    "coc.nvim": "^0.0.79",
    "immer": "^7.0.14",
    "lodash": "^4.17.20",
    "neovim": "^4.9.0",
    "printf": "^0.6.0",
    "redux": "^4.0.5",
    "strip-ansi": "^6.0.0",
    "type-fest": "^0.18.0",
    "uuid": "^8.3.1",
    "vscode-languageserver-types": "^3.15.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@jest/types": "^26.6.1",
    "@types/ansi-escape-sequences": "^4.0.0",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.6",
    "@types/prettier": "^2.1.5",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.24",
    "@types/webpack-merge": "^4.1.5",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-merge": "^5.3.0"
  }
}
